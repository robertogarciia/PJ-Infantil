<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bosque_Image_View"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="1450dp"
        android:layout_height="900dp"
        app:srcCompat="@drawable/bosque_hadas_colores_vivos_445983_743"
        tools:layout_editor_absoluteX="-109dp"
        tools:layout_editor_absoluteY="-19dp" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="990dp"
        android:layout_height="629dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backwhite"
        tools:layout_editor_absoluteX="143dp" />

    <ImageView
        android:id="@+id/BackGroundColorPrincipal"
        android:layout_width="1123dp"
        android:layout_height="560dp"
        app:srcCompat="@drawable/amarillo"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageView
        android:id="@+id/BackgroundWhitePickColors"
        android:layout_width="1646dp"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BackGroundColorPrincipal"
        app:srcCompat="@drawable/backwhite" />


    <ImageView
        android:id="@+id/PickColorBlue"
        android:layout_width="287dp"
        android:layout_height="165dp"
        app:srcCompat="@drawable/botoblau"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="612dp" />

    <ImageView
        android:id="@+id/PickColorGreen"
        android:layout_width="282dp"
        android:layout_height="155dp"
        app:srcCompat="@drawable/botonverde"
        tools:layout_editor_absoluteX="364dp"
        tools:layout_editor_absoluteY="617dp" />

    <ImageView
        android:id="@+id/PickColorRed"
        android:layout_width="270dp"
        android:layout_height="160dp"
        app:srcCompat="@drawable/botonrojo"
        tools:layout_editor_absoluteX="661dp"
        tools:layout_editor_absoluteY="612dp" />

    <ImageView
        android:id="@+id/PickColorPurple"
        android:layout_width="270dp"
        android:layout_height="160dp"
        app:srcCompat="@drawable/botomorado"
        tools:layout_editor_absoluteX="942dp"
        tools:layout_editor_absoluteY="612dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="603dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="2dp" />


</androidx.constraintlayout.widget.ConstraintLayout>